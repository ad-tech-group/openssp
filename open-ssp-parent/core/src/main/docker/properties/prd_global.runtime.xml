<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<!-- trigger expression for the cache jobs -->
	<entry key="cache-trigger-expression"><![CDATA[0 0/5 * * * ?]]></entry>
	
	<!-- factor to reduce the winnerbid price in case of only one bidder and no floor price -->
	<!-- default is 0.3 -->
	<entry key="draw-moderation">0.3</entry>
	
	<!-- the timeout in ms for execution the executor service -->
	<!-- default is 500ms --> 
	<entry key="cdl-timeout">1200</entry>

	<!-- url of the data provider where the cache is updated from -->
	<entry key="data-provider-scheme">https</entry>

	<entry key="data-provider-host">sspservices.pub.network</entry>

	<entry key="data-provider-port">443</entry>

	<!-- credential for remote access -->
	<entry key="master_pw">frogs</entry>

	<!-- credential for remote access -->
	<entry key="master_user">izod</entry>

    <entry key="validator-handler-for-video-class">com.atg.openssp.core.entry.video.VideoObjectEntryValidatorHandler</entry>

    <entry key="validator-handler-for-banner-class">com.atg.openssp.core.entry.banner.BannerObjectEntryValidatorHandler</entry>

	<entry key="validator-handler-for-header-class">com.atg.openssp.core.entry.header.HeaderBiddingEntryValidatorHandler</entry>

	<entry key="bid-request-builder-handler-for-video-class">com.atg.openssp.core.exchange.VideoObjectBidRequestBuilderHandler</entry>

	<entry key="bid-request-builder-handler-for-banner-class">com.atg.openssp.core.exchange.BannerObjectBidRequestBuilderHandler</entry>

	<entry key="bid-request-builder-handler-for-header-class">com.atg.openssp.core.exchange.HeaderBiddingBidRequestBuilderHandler</entry>

	<entry key="adserver-broker-handler-class">io.freestar.ssp.channel.adserving.FreestarAdserverBrokerHandler</entry>

	<entry key="cookie-sync-handler-class">io.freestar.openssp.common.exchange.aerospike.EnvironmentAerospikeCookieSyncHandler</entry>

</properties>
